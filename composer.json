{
  "name": "Ffuenf/Ffuenf_Pagespeed",
  "license": "MIT",
  "description": "This is a extension for Magento Community Edition that adds support for ngx_pagespeed to the cache clearing mechanisms.",
  "authors": [
    {
      "name": "Achim Rosenhagen",
      "email": "a.rosenhagen@ffuenf.de"
    }
  ],
  "type": "magento-module",
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.firegento.com"
    }
  ],
  "extra": {
    "magento-root-dir": "./magento/",
    "map": [
      [
        "app/code/community/Ffuenf/Pagespeed",
        "app/code/community/Ffuenf/Pagespeed"
      ],
      [
        "app/etc/modules/Ffuenf_Pagespeed.xml",
        "app/etc/modules/Ffuenf_Pagespeed.xml"
      ]
    ]
  }
}